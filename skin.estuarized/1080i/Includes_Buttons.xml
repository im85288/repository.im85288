	</include>
	<include name="SettingsItemCommon">
		<texturenofocus border="40">buttons/dialogbutton-nofo.png</texturenofocus>
		<texturefocus border="40" colordiffuse="$VAR[ColourFocus]">buttons/dialogbutton-fo.png</texturefocus>
